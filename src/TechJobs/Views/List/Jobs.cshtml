<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{

    @foreach (var @row in @ViewBag.jobs)
    {
        <table class="job-listing">
            @foreach (@KeyValuePair<string, string> item in @row)
            {
                <tr>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                </tr>
            }
        </table>
    }
}